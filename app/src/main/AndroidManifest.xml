<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlinecourse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Student_Details"
            android:theme="@style/Theme.AppCompat.Light.Dialog"
            android:label="@string/student_info"/>
        <activity android:name=".Student_Info" />
        <activity android:name=".CourseInformation" />
        <activity android:name=".TeacherOrStudent" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp_Teacher" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SignUp_Student" />
        <activity
            android:name=".ui.login.LoginActivity"/>
    </application>

</manifest>